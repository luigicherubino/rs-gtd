		</Resource>
		<Resource> <Name>/nextactions</Name>
			<Type>WebService</Type>
			<OperationList>
				<Operation>
					<Name>Next Actions</Name>
					<Type>GET</Type>
					<CompositionList>
						<!--Composition>
							<Name>Commands</Name>
							<Type>Command</Type>
							<OptionList>
								<Option>
									<Uri>/stuff</Uri>
									<Operation>POST</Operation>
								</Option> 
							</OptionList>
						</Composition-->
						<Composition>
							<Name>Next Action List</Name>
							<Class>next-action-list</Class>
							<TemplateVar>Content</TemplateVar>
							<Type>Query</Type>
							<QueryType>List</QueryType>
							<Table>NEXT_ACTION_LIST</Table>
							<ColumnList>
								<Column>
									<Class>rs-h1</Class>
									<Name>Action</Name>
									<Value>NEXT_ACTION</Value>
									<Href>/nextactions/{id}</Href>
								</Column>
									<Column>
									<Name>Outcome</Name>
									<Value>OUTCOME</Value>
								</Column>
								<Column>
									<Value>Description</Value>
								</Column>
								<Column>
									<Value>Context</Value>
								</Column>
								<Column>
									<Type>Options</Type>
									<OptionList>
										<Option>
											<Uri>/nextactions/{id}</Uri>
											<Operation>DELETE</Operation>
											<UriValues>rs-test</UriValues>
										</Option>
										<Option>
											<Uri>/nextactions/{id}</Uri>
											<Operation>GET</Operation>
										</Option>
									</OptionList>
								</Column>
							</ColumnList>
						</Composition>
					</CompositionList>
				</Operation>
				<Operation>
					<Name>Process</Name>
					<Aggregate>ProcessAction</Aggregate>
					<Command>ActionStuff</Command>
					<Type>POST</Type>
					<Redirect>/nextactions/{id}</Redirect>
                    <View>stuff_list</View>
                    <FilterList>
                        <Filter>
                            <Field>stuff_id</Field>
                            <Value>{query:stuff_id}</Value>
                        </Filter>
                    </FilterList>
					<ControlList>
						<Control>
							<Name>stuff-id</Name>
							<Type>hidden</Type>
							<Value>{stuff_id}</Value>
						</Control>
						<Control>
							<Label>Description</Label>
							<Name>Description</Name>
							<Value>{Description}</Value>
							<Type>text</Type>
						</Control>
						<Control>
							<Label>Context</Label>
							<Name>context</Name>
							<Type>select</Type>
							<View>context_list</View>
							<Option>context</Option>
							<Value>{context}</Value>
							<FilterColumn>context</FilterColumn>
							<FilterValue>is not null</FilterValue>
						</Control>
						<Control>
							<Label>What's the successfull outcome?</Label>
							<Name>outcome</Name>
							<Type>text</Type>
						</Control>
						<Control>
							<Label>What is the Next Action?</Label>
							<Name>next-action</Name>
							<Type>text</Type>
						</Control>
						<Control>
							<Label>Time</Label>
							<Name>time</Name>
							<Type>text</Type>
						</Control>
						<Control>
							<Label>Energy</Label>
							<Name>energy</Name>
							<Type>text</Type>
						</Control>
						<Control>
							<Label>Priority</Label>
							<Name>priority</Name>
							<Type>text</Type>
