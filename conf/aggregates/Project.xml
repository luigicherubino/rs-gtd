<?xml version="1.0"?>
<ProjectAggregate>
	<Entity>Project</Entity>
	<ActionList>
		<Action> <Command>CreateProject</Command>
			<Type>Init</Type>
			<Id>ProjectId</Id>
			<InitMapList type="list">
				<InitMap name="ProjectId">ProjectId</InitMap>
				<InitMap name="Name">Name</InitMap>
				<InitMap name="Purpose">Purpose</InitMap>
				<InitMap name="Vision">Vision</InitMap>
				<InitMap name="IsActive">IsActive</InitMap>
				<InitMap name="FolderId">FolderId</InitMap>
			</InitMapList>
		</Action>
		<Action> <Command>UpdateProject</Command>
			<Type>Update Project</Type>
			<MapList type="list">
				<Map name="ProjectId">ProjectId</Map>
				<Map name="Name">Name</Map>
				<Map name="Purpose">Purpose</Map>
				<Map name="Vision">Vision</Map>
				<Map name="IsActive">IsActive</Map>
				<Map name="FolderId">FolderId</Map>
			</MapList>
		</Action>
		<Action> <Command>CreateProjectOutcome</Command>
			<Type>Add Outcome</Type>
			<MapList type="list">
				<Map name="OutcomeId">OutcomeId</Map>
				<Map name="Name">Name</Map>
				<Map name="Status">Status</Map>
				<Map name="Comments">Comments</Map>
			</MapList>
		</Action>
		<Action> <Command>UpdateProjectOutcome</Command>
			<Type>Update Outcome</Type>
			<MapList type="list">
				<Map name="OutcomeId">OutcomeId</Map>
				<Map name="Name">Name</Map>
				<Map name="Status">Status</Map>
				<Map name="Comments">Comments</Map>
			</MapList>
		</Action>
		<Action> <Command>DeactivateProjectOutcome</Command>
			<Type>Delete Outcome</Type>
			<MapList type="list">
				<Map name="OutcomeId">OutcomeId</Map>
				<Map name="Reason">Reason</Map>
			</MapList>
		</Action>
	</ActionList>
</ProjectAggregate>
