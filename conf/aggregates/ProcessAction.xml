<?xml version="1.0"?>
<ProcessAction>
	<Entity>ProcessAction</Entity>
	<ActionList>
		<Action>
			<Command>ActionStuff</Command>
			<Type>Init</Type>
			<Id>ActionId</Id>
			<InitMapList type="list">
				<InitMap id="ActionId">ActionId</InitMap>
				<InitMap id="StuffId">StuffId</InitMap>
				<InitMap id="Description">Description</InitMap>
				<InitMap id="Context">Context</InitMap>
				<InitMap id="Outcome">Outcome</InitMap>
				<InitMap id="NextAction">NextAction</InitMap>
				<InitMap id="Time">Time</InitMap>
				<InitMap id="Energy">Energy</InitMap>
				<InitMap id="Priority">Priority</InitMap>
				<InitMap id="Comments">Comments</InitMap>
			</InitMapList>
		</Action>
		<Action>
			<Command>AssignTaskToProject</Command>
			<Type>Update ProcessAction</Type>
			<MapList type="list">
				<Map name="ActionId">ActionId</Map>	
				<Map name="ProjectId">ProjectId</Map>	
			</MapList>
		</Action>
		<Action>
			<Command>CancelAction</Command>
			<Type>Update ProcessAction</Type>
			<MapList type="list">
				<Map name="ActionId">ActionId</Map>	
				<Map name="Date">DateCanceled</Map>	
			</MapList>
		</Action>
		<Action>
			<Command>UpdateAction</Command>
			<Type>Update ProcessAction</Type>
			<MapList type="list">
				<Map name="ActionId">ActionId</Map>	
				<Map name="context">Context</Map>	
				<Map name="Outcome">Outcome</Map>	
				<Map name="NextAction">NextAction</Map>	
				<Map name="Time">Time</Map>	
				<Map name="Energy">Energy</Map>	
				<Map name="Priority">Priority</Map>	
				<Map name="Comments">Comments</Map>	
			</MapList>
		</Action>
	</ActionList>
</ProcessAction>
