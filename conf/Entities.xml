<?xml version="1.0"?>
<Entities>
	<EntityList>
		<Entity> <Name>Context</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property>
					<Name>Context</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>IsActive</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
			<EventList>
				<Event> <Name>ContextCreated</Name>
					<Trigger>OnCreated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="Context">Context</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
						<PayLoad id="IsActive">IsActive</PayLoad>
					</PayLoadList>
				</Event>
				<Event> <Name>ContextUpdated</Name>
					<Trigger>OnPayloadUpdated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="Context">Context</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
					</PayLoadList>
				</Event>
			</EventList>
		</Entity>
		
		<Entity> <Name>Folder</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property> <Name>FolderId</Name> <Type>String</Type> </Property>
				<Property> <Name>Name</Name> <Type>String</Type> </Property>
				<Property> <Name>Comments</Name> <Type>String</Type> </Property>
				<Property> <Name>IsActive</Name> <Type>String</Type> </Property>
			</PropertyList>
			<EventList>
				<Event> <Name>FolderCreated</Name>
					<Trigger>OnCreated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="FolderId">FolderId</PayLoad>
						<PayLoad id="Name">Name</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
						<PayLoad id="IsActive">IsActive</PayLoad>
					</PayLoadList>
				</Event>
				<Event> <Name>FolderUpdated</Name>
					<Trigger>OnPayloadUpdated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="FolderId">FolderId</PayLoad>
						<PayLoad id="Name">Name</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
						<PayLoad id="IsActive">IsActive</PayLoad>
					</PayLoadList>
				</Event>
			</EventList>
		</Entity>

		<Entity> <Name>Project</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property>
					<Name>ProjectId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Name</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Purpose</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Vision</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>IsActive</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>FolderId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Tasks</Name>
					<Type>ProjectTask[0..*]</Type>
				</Property>
				<Property>
					<Name>Outcome</Name>
					<Type>ProjectOutcome[0..*]</Type>
				</Property>
			</PropertyList>
			<EventList>
				<Event> <Name>ProjectCreated</Name>
					<Trigger>OnCreated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="ProjectId">ProjectId</PayLoad>
						<PayLoad id="Name">Name</PayLoad>
						<PayLoad id="Uri">{uri:Name}</PayLoad>
						<PayLoad id="Purpose">Purpose</PayLoad>
						<PayLoad id="Vision">Vision</PayLoad>
						<PayLoad id="FolderId">FolderId</PayLoad>
						<PayLoad id="IsActive">IsActive</PayLoad>
					</PayLoadList>
				</Event>
				<Event> <Name>ProjectUpdated</Name>
					<Trigger>OnPayloadUpdated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="ProjectId">ProjectId</PayLoad>
						<PayLoad id="Name">Name</PayLoad>
						<PayLoad id="Uri">{uri:Name}</PayLoad>
						<PayLoad id="Purpose">Purpose</PayLoad>
						<PayLoad id="Vision">Vision</PayLoad>
						<PayLoad id="FolderId">FolderId</PayLoad>
						<PayLoad id="IsActive">IsActive</PayLoad>
					</PayLoadList>
				</Event>
                <Event> <Name>ProjectOutcomeCreated</Name>
                    <Trigger>OnAdded</Trigger>
                    <Property>Outcome</Property>
                    <PayLoadList type="list">
                        <PayLoad id="ProjectId">{parent:ProjectId}</PayLoad>
                        <PayLoad id="OutcomeId">OutcomeId</PayLoad>
                        <PayLoad id="Name">Name</PayLoad>
                        <PayLoad id="Status">Status</PayLoad>
                        <PayLoad id="Comments">Comments</PayLoad>
                    </PayLoadList>
                </Event>
                <Event> <Name>ProjectOutcomeDeactivated</Name>
                    <Trigger>OnDeleted</Trigger>
                    <Property>Outcome</Property>
                    <PayLoadList type="list">
                        <PayLoad id="OutcomeId">OutcomeId</PayLoad>
                        <PayLoad id="ProjectId">{parent:ProjectId}</PayLoad>
                    </PayLoadList>
                </Event>
			
			</EventList>
		</Entity>

		<Entity> <Name>ProjectTask</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property> <Name>TaskId</Name> <Type>String</Type> </Property>
			</PropertyList>
			<ColaborationList>
				<Colaboration>
					<Entity>Project</Entity>
					<Type>Transaction - Specific</Type>
				</Colaboration>
			</ColaborationList>
		</Entity>

		<Entity> <Name>ProjectOutcome</Name>
			<Type>Generic Item</Type>
			<Key>OutcomeId</Key>
			<PropertyList>
				<Property>
					<Name>OutcomeId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Name</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Status</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Reason</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
			<EventList>
                <Event> <Name>ProjectOutcomeUpdated</Name>
                    <Trigger>OnPayloadUpdated</Trigger>
                    <PayLoadList type="list">
                        <PayLoad id="ProjectId">{parent:ProjectId}</PayLoad>
                        <PayLoad id="OutcomeId">OutcomeId</PayLoad>
                        <PayLoad id="Name">Name</PayLoad>
                        <PayLoad id="Status">Status</PayLoad>
                        <PayLoad id="Comments">Comments</PayLoad>
					</PayLoadList>
                </Event>
			</EventList>
            <ColaborationList>
                <Colaboration>
                    <Entity>Project</Entity>
                    <Type>Transaction - Specific</Type>
				</Colaboration>
			</ColaborationList>
		</Entity>		

		<Entity> <Name>ProcessStuff</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Eliminated</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Status</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>When</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
			<EventList>
				<Event>
					<Name>StuffCollected</Name>
					<Trigger>OnCreated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="StuffId">StuffId</PayLoad><!-- BUG : Need to reverse map-->
						<PayLoad id="Description">Description</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
						<PayLoad id="When">When</PayLoad>
					</PayLoadList>
				</Event>
				<Event>
					<Name>StuffUpdated</Name>
					<Trigger>OnPayloadUpdated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="StuffId">StuffId</PayLoad>
						<PayLoad id="Description">Description</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
					</PayLoadList>
				</Event>
				<Event>
					<Name>StuffEliminated</Name>
					<Trigger>OnPropertyUpdated</Trigger>
					<Property>Status</Property>
					<PropertyValue>Eliminated</PropertyValue>
					<PayLoadList type="list">
						<PayLoad id="StuffId">StuffId</PayLoad>
					</PayLoadList>
				</Event>
				</EventList>
		</Entity>

		<Entity> <Name>ProcessAction</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property> <Name>ActionId</Name>
					<Type>String</Type>
				</Property>
				<Property> <Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property> <Name>ProjectId</Name>
					<Type>String</Type>
				</Property>
				<Property> <Name>Status</Name>
					<Type>String</Type>
				</Property>
				<Property> <Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Context</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Outcome</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>NextAction</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Time</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Energy</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Priority</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>DateDone</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>DateCanceled</Name>
					<Type>String</Type>
				</Property>
					<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
			<EventList>
				<Event>
					<Name>ActionDone</Name>
					<Trigger>OnUpdate DateDone</Trigger>
					<PayLoadList type="list">
						<PayLoad id="ActionId">ActionId</PayLoad>
						<PayLoad id="ProjectId">ProjectId</PayLoad>
					</PayLoadList>
				</Event>
				<Event>
					<Name>ActionCanceled</Name>
					<Trigger>OnPropertySet</Trigger>
					<Property>DateCanceled</Property>
					<PayLoadList type="list">
						<PayLoad id="ActionId">ActionId</PayLoad>
						<PayLoad id="ProjectId">ProjectId</PayLoad>
						<PayLoad id="Date">DateCanceled</PayLoad>
					</PayLoadList>
				</Event>
				<Event>
					<Name>StuffActioned</Name>
					<Trigger>OnCreated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="ActionId">ActionId</PayLoad>
						<PayLoad id="StuffId">StuffId</PayLoad>
						<PayLoad id="ProjectId">ProjectId</PayLoad>
						<PayLoad id="Description">Description</PayLoad>
						<PayLoad id="Context">Context</PayLoad>
						<PayLoad id="Outcome">Outcome</PayLoad>
						<PayLoad id="NextAction">NextAction</PayLoad>
						<PayLoad id="Time">Time</PayLoad>
						<PayLoad id="Energy">Energy</PayLoad>
						<PayLoad id="Priority">Priority</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
					</PayLoadList>
				</Event>
				<Event> <Name>TaskAssignedToProject</Name>
					<Trigger>OnPropertyUpdated</Trigger>
					<Property>ProjectId</Property>
					<PayLoadList type="list">
						<PayLoad id="ActionId">ActionId</PayLoad>
						<PayLoad id="ProjectId">ProjectId</PayLoad>
					</PayLoadList>
				</Event>
				<Event> <Name>ActionUpdated</Name>
					<Trigger>OnPayloadUpdated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="ActionId">{ActionId}</PayLoad>
						<PayLoad id="ProjectId">{ProjectId}</PayLoad>
						<PayLoad id="Description">Description</PayLoad>
						<PayLoad id="Context">Context</PayLoad>
						<PayLoad id="Outcome">Outcome</PayLoad>
						<PayLoad id="NextAction">NextAction</PayLoad>
						<PayLoad id="Time">Time</PayLoad>
						<PayLoad id="Energy">Energy</PayLoad>
						<PayLoad id="Priority">Priority</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
					</PayLoadList>
				</Event>
			</EventList>
		</Entity>

		<Entity> <Name>SomeDayTask</Name>
			<Type>Generic Item</Type>
			<PropertyList>
				<Property>
					<Name>TaskId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Status</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Context</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>TickleDate</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Eliminated</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
			<EventList>
				<Event>
					<Name>StuffIncubated</Name>
					<Trigger>OnCreated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="TaskId">TaskId</PayLoad>
						<PayLoad id="StuffId">StuffId</PayLoad>
						<PayLoad id="Context">Context</PayLoad>
						<PayLoad id="Description">Description</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
						<PayLoad id="TickleDate">TickleDate</PayLoad>
					</PayLoadList>
				</Event>
				<Event>
					<Name>SomeDayUpdated</Name>
					<Trigger>OnPayloadUpdated</Trigger>
					<PayLoadList type="list">
						<PayLoad id="TaskId">TaskId</PayLoad>
						<PayLoad id="Description">Description</PayLoad>
						<PayLoad id="Context">Context</PayLoad>
						<PayLoad id="Comments">Comments</PayLoad>
						<PayLoad id="TickleDate">TickleDate</PayLoad>
					</PayLoadList>
				</Event>
			</EventList>
		</Entity>
	</EntityList>
</Entities>
