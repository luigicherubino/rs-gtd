<?xml version="1.0"?>
<GTDCommands>
	<Name>GTD Commands</Name>
	<CommandList>
		<Command> <Name>CreateContext</Name>
			<PropertyList>
				<Property>
					<Name>Context</Name>
				</Property>
				<Property>
					<Name>Comments</Name>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>DeactivateContext</Name>
			<PropertyList>
				<Property>
