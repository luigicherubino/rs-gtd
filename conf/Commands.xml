				</Property>
				<Property>
					<Name>Time</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Energy</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Priority</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
