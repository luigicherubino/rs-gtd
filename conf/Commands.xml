<?xml version="1.0"?>
<GTDCommands>
	<Name>GTD Commands</Name>
	<CommandList>
		<Command> <Name>CreateContext</Name>
			<PropertyList>
				<Property>
					<Name>Context</Name>
				</Property>
				<Property>
					<Name>Comments</Name>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>DeactivateContext</Name>
			<PropertyList>
				<Property>
					<Name>Context</Name>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>CollectStuff</Name>
			<Description>Collect everything, outside your head</Description>
			<PropertyList>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
					<Default>uid</Default>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>When</Name>
					<Type>DateTime</Type>
					<Default>{now}</Default>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>UpdateStuff</Name>
			<PropertyList>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Context</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>EliminateStuff</Name>
			<PropertyList>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>IncubateStuff</Name>
			<PropertyList>
				<Property>
					<Name>TaskId</Name>
					<Type>String</Type>
					<Default>uid</Default>
				</Property>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Context</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>TickleDate</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>ActionStuff</Name>
			<PropertyList>
				<Property>
					<Name>ActionId</Name>
					<Type>String</Type>
					<Default>uid</Default>
				</Property>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Context</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Outcome</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>NextAction</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Time</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Energy</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Priority</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>DoAction</Name>
			<PropertyList>
				<Property>
					<Name>ActionId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Date</Name>
					<Type>String</Type>
				</Property>
				</PropertyList>
		</Command>
	</CommandList>
</GTDCommands>
