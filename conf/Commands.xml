				</Property>
			</PropertyList>
		</Command>

		<Command> <Name>CollectStuff</Name>
			<Description>Collect everything, outside your head</Description>
			<PropertyList>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
					<Default>uid</Default>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>When</Name>
					<Type>DateTime</Type>
					<Default>{now}</Default>
				</Property>
			</PropertyList>
		</Command>
		<Command> <Name>UpdateStuff</Name>
			<PropertyList>
				<Property>
					<Name>StuffId</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Description</Name>
					<Type>String</Type>
				</Property>
				<Property>
					<Name>Comments</Name>
					<Type>String</Type>
				</Property>
			</PropertyList>
		</Command>
